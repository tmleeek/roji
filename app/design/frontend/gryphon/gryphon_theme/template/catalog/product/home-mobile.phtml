        var keycode = (event.keyCode ? event.keyCode : event.which);
        if(keycode == '13') 
          $('.home-page-mobile-subscribe-form').find('.subscribe_newsletter').trigger('click');
      });
      $('.home-page-mobile-subscribe-form').find('.subscribe_newsletter').click(function(e){
        e.preventDefault();        
        var subscribe_email = $(this).prev('.subscribe_email').val();

        if(subscribe_email!=="" && subscribe_email!=="Enter Your Email Address") {
          $('.newsletter_ajax_loader').show();

          var request = $.ajax({
              url: "<?php echo $this->getUrl('discovertea/index/subscribe', array('_secure'=>true)) ?>",
              method: "POST",
              data: { subscribe_email : subscribe_email },
              dataType: "html"
          });
           
          request.done(function( msg ) {
            var message = JSON.parse(msg);
            console.log(message);
            $('.newsletter_ajax_loader').hide();
            if(message.error_messages)
              $('span.ajax_msg').html('<p>'+message.error_messages+'</p>').show().delay(5000).fadeOut();              
              else
              $('span.ajax_msg').html('<p>Successfully subscribed to newsletter</p>').show().delay(5000).fadeOut();
          });
        }        
      });

      $('#home-mobile-save-layering').click(function(e){
        e.preventDefault();
        var pidvalue1 = jQuery(".graph-selection-tea-mobile .graph-selection-name #graph-selection-name-01").attr('data-id');
        var pidvalue2 = jQuery(".graph-selection-tea-mobile .graph-selection-name #graph-selection-name-01").attr('data-id');
        //alert(pidvalue1);alert(pidvalue2);
        // jQuery("#successmsg").hide();
        // jQuery("#ajax_loadermsg").show();    
        console.log(pidvalue1)
        console.log(pidvalue2)
        var cid = '<?php echo $cid ?>';
        var pid = pidvalue1+'|||'+pidvalue2;
        var dataString = 'cid='+ cid + '&pid='+ pid;
              jQuery.ajax({
                  type: "POST",
                  data: dataString,
                  url: "<?php echo Mage::getUrl('savelayering/index/ajax'); ?>"
              }).done(function(result) { 
                // jQuery("#ajax_loadermsg").hide();
                // jQuery("#successmsg").show();
                // jQuery("#successmsg").html(''+result+'');        
                alert(result);
              });
      });
    });
  </script>